<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('partials/head') %>
        <%- include('index/head-index') %>
</head>

<body>

    <header >
        <%- include('partials/navbar'); %>
    </header>

    <main class="main-home">
        <div class="row h-100 align-items-start px-2">

            <div class="text-primary">
                <h1 class="display-1 fw-bold text-center my-5">MyClimate</h1>

                <p class="text-center my-5" style="max-width:500px;margin:auto;">
                    climate predictions for your home
                </p>

                <div style="max-width:700px;margin:auto;">
                    <input type="text" id="address-search" class="form-control" name="address-text"
                        placeholder="Enter a NSW street address ..." autocomplete="off">
                    <form action="./data-overview" method="get">
                        <!-- <input type="text" id="address-search" name="address" placeholder="Enter a NSW street address."> -->
                        <input type="hidden" id="address_id" name="address">
                        <input id='get-address' class="btn btn-primary my-2" type="submit" value="Submit">
                    </form>

                </div>

            </div>
    </main>

    <footer>
        <%- include('partials/footer'); %>
    </footer>

    <script>
        autocompleteAddress('address-search')
    </script>

</body>

</html>